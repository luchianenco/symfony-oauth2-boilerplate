{
  "name": "arbeits-platten",
  "version": "0.1.0",
  "description": "Arbeits Platten Configurator",
  "main": "src/AppBundle/Resources/public/js/index.js",
  "repository": "git@bitbucket.org:freshcells/arbeitsplatten-web.git",
  "author": "Freshcells by Serghei Luchianenco (serghei.luchianenco@freshcells.de)",
  "license": "UNLICENSED",
  "scripts": {
    "test": "karma start --single-run --ci --test",
    "tdd": "karma start --auto-watch --test",
    "clean": "rm -Rf ./web/assets",
    "develop": "webpack --watch --config webpack.config.babel.js --progress",
    "build": "node_modules/webpack/bin/webpack.js",
    "backend-hot": "rm -Rf ./web/assets && webpack --watch --config webpack.config.babel.js --progress --useServerPath --disableLinting",
    "frontend-hot": "webpack-dev-server --watch --config webpack.config.babel.js --progress --useServerPath --hot",
    "dist": "rm -Rf ./web/assets && webpack --config webpack.config.babel.js --optimize-minimize --optimize-occurence-order --progress --production",
    "dist-debug": "rm -Rf ./web/assets && webpack --config webpack.config.babel.js --optimize-occurence-order --progress",
    "lint": "eslint src"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-runtime": ">=6.0.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.8",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "^0.9.0",
    "flow": "^0.2.3",
    "flow-bin": "0.32.0",
    "flow-status-webpack-plugin": "^0.1.4",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-junit-reporter": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.1",
    "webpack": "2.2.0"
  }
}
